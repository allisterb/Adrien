{
	public partial class Tensor
	{
		<#for (int i = 2; i <= 8; i++) {#>  
		public TensorIndexExpression this[<#for (int j = 1; j < i; j++) {#>Index index<#=j#>, <#}#>Index index<#=i#>, 
		<#if(i > 1){#>(<#for (int j = 1; j < i; j++) {#>Dimension n<#=j#>, <#}#>Dimension n<#=i#>) <#}#><#else{#>Dimension <#}#>N]		
